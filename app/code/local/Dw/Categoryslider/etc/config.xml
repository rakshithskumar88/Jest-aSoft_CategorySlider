<?xml version="1.0"?>
<config>
 <modules>
  <Dw_Categoryslider>
   <version>0.1.0</version>
  </Dw_Categoryslider>
 </modules>
 <frontend>
  <routers>
   <categoryslider>
     <use>standard</use>
      <args>
       <module>Dw_Categoryslider</module>
       <frontName>categoryslider</frontName>
      </args>
    </categoryslider>
  </routers>
 <layout>
    <updates>
     <categoryslider>
