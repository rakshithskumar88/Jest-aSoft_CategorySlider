     </categoryslider>
     </entities>
    </categoryslider_mysql4>
 </models>
 <resources>
   <categoryslider_setup>
    <setup>
     <module>Dw_Categoryslider</module>
    </setup>
    <connection>
     <use>core_setup</use>
    </connection>
   </categoryslider_setup>
 <categoryslider_write>
    <connection>
     <use>core_write</use>
    </connection>
 </categoryslider_write>
 <categoryslider_read>
   <connection>
    <use>core_read</use>
   </connection>
 </categoryslider_read>
 </resources>
 <blocks>
  <categoryslider>
    <class>Dw_Categoryslider_Block</class>
